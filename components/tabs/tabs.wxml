<view class="tabs">
  <view class="tabs-nav">
    <view class="nav-item  {{item.isActive ? 'active' : ''}}" wx:for="{{tabs}}" wx:key="id" bindtap="handleNavClick" data-index="{{index}}">
    <text>{{item.text}}</text>
    </view>
  </view>
  <view class="tabs-plant">
    <slot></slot>
  </view>
</view>
