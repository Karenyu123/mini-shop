.detail-footer {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  z-index: 1;
  display: flex;
  text-align: center;
}
.detail-footer .left {
  flex: 50%;
  background: #ffffff;
  display: flex;
  color: #666666;
  padding-top: 12rpx;
}
.detail-footer .left .tool-item {
  position: relative;
  flex: 1;
}
.detail-footer .left .tool-item .btn {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  opacity: 0;
}
.detail-footer .left .tool-item .iconfont {
  font-size: 40rpx;
}
.detail-footer .left .tool-item .text {
  font-size: 24rpx;
}
.detail-footer .add-cart {
  flex: 25%;
  background: #FFA500;
  line-height: 100rpx;
  color: white;
}
.detail-footer .buy {
  flex: 25%;
  background: var(--themeColor);
  line-height: 100rpx;
  color: white;
}
